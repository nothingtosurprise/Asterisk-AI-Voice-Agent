{
  "id": null,
  "title": "Reliability",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "5s",
  "templating": {
    "list": [
      {
        "name": "call",
        "label": "Call",
        "type": "query",
        "query": "label_values(ai_agent_streaming_active, call_id)",
        "includeAll": true,
        "multi": false
      }
    ]
  },
  "panels": [
    {
      "type": "stat",
      "title": "Target Health (ai-engine)",
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
      "targets": [
        {"expr": "up{job='ai-engine'}", "refId": "A"}
      ]
    },
    {
      "type": "stat",
      "title": "Streaming Fallbacks (5m)",
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
      "targets": [
        {"expr": "sum(rate(ai_agent_streaming_fallbacks_total{call_id=~\"$call\"}[5m])) or vector(0)", "refId": "A"}
      ]
    },
    {
      "type": "stat",
      "title": "Keepalive Timeouts (5m)",
      "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
      "targets": [
        {"expr": "sum(rate(ai_agent_streaming_keepalive_timeouts_total{call_id=~\"$call\"}[5m])) or vector(0)", "refId": "A"}
      ]
    },
    {
      "type": "stat",
      "title": "Barge-in Events (5m)",
      "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
      "targets": [
        {"expr": "sum(rate(ai_agent_barge_in_events_total{call_id=~\"$call\"}[5m])) or vector(0)", "refId": "A"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Streaming Fallbacks (rate)",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
      "targets": [
        {"expr": "sum by (call_id) (rate(ai_agent_streaming_fallbacks_total{call_id=~\"$call\"}[$__rate_interval])) or vector(0)", "refId": "A"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Keepalive Timeouts (rate)",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
      "targets": [
        {"expr": "sum by (call_id) (rate(ai_agent_streaming_keepalive_timeouts_total{call_id=~\"$call\"}[$__rate_interval])) or vector(0)", "refId": "A"}
      ]
    },
    {
      "type": "timeseries",
      "title": "AudioSocket RX Bytes (rate)",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
      "targets": [
        {"expr": "rate(ai_agent_audiosocket_rx_bytes_total[$__rate_interval])", "refId": "A"}
      ]
    },
    {
      "type": "timeseries",
      "title": "AudioSocket TX Bytes (rate)",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
      "targets": [
        {"expr": "rate(ai_agent_audiosocket_tx_bytes_total[$__rate_interval])", "refId": "A"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Stream RX Bytes per call (rate)",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
      "targets": [
        {"expr": "sum by (call_id) (rate(ai_agent_stream_rx_bytes_total{call_id=~\"$call\"}[$__rate_interval])) or vector(0)", "refId": "A"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Stream TX Bytes per call (rate)",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
      "targets": [
        {"expr": "sum by (call_id) (rate(ai_agent_stream_tx_bytes_total{call_id=~\"$call\"}[$__rate_interval])) or vector(0)", "refId": "A"}
      ]
    }
  ]
}
