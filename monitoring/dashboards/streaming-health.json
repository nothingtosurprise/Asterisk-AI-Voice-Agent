{
  "id": null,
  "title": "Streaming Health",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "type": "stat",
      "title": "Active Streams",
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
      "targets": [
        {"expr": "sum(ai_agent_streaming_active)", "refId": "A"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Jitter Buffer Depth (chunks)",
      "gridPos": {"h": 8, "w": 12, "x": 6, "y": 0},
      "targets": [
        {"expr": "avg(ai_agent_streaming_jitter_buffer_depth)", "refId": "A"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Streaming Fallbacks (rate)",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
      "targets": [
        {"expr": "rate(ai_agent_streaming_fallbacks_total[5m])", "refId": "A"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Keepalive Timeouts (rate)",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
      "targets": [
        {"expr": "rate(ai_agent_streaming_keepalive_timeouts_total[5m])", "refId": "A"}
      ]
    }
  ]
}
